{
    "name": "dream/usos",
    "type": "project",
    "repositories": [
        {"type": "vcs", "url": "https://github.com/dream-group/dream-apply-sdk"}
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": false
        },
        "platform": {
            "php": "7.2.5"
        }
    },
    "autoload": {
        "psr-4": {
            "Dream\\USOS\\": "app/classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dream\\USOS\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">= 7.2.5",
        "ext-json": "*",
        "dream-group/dream-apply-sdk": "^2.0.1 || ^3",
        "guzzlehttp/guzzle": "^7.5",
        "nyholm/psr7": "^1.7",
        "php-di/php-di": "^6.3.5",
        "php-di/slim-bridge": "^3.3",
        "slim/http": "^1.2",
        "slim/slim": "^4.8.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^8.5.33",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "*"
    }
}
