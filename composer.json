{
    "name": "dream/usos",
    "type": "project",
    "repositories": [
        {"type": "vcs", "url": "https://github.com/dream-group/dream-apply-sdk"}
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.2.5"
        }
    },
    "autoload": {
        "psr-4": {
            "Dream\\USOS\\": "app/classes"
        }
    },
    "require": {
        "php": ">= 7.2.5",
        "ext-json": "*",
        "dream-group/dream-apply-sdk": "^1",
        "league/uri-schemes": "^1.2",
        "silex/silex": "^2.3"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "phpcompatibility/php-compatibility": "*",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "*"
    }
}
