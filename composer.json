{
    "name": "dream/usos",
    "type": "project",
    "repositories": [
        {"type": "vcs", "url": "https://github.com/dream-group/dream-apply-sdk"}
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.2.5"
        }
    },
    "autoload": {
        "psr-4": {
            "Dream\\USOS\\": "app/classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dream\\USOS\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">= 7.2.5",
        "ext-json": "*",
        "dream-group/dream-apply-sdk": "^1.7.1",
        "nyholm/psr7": "^1.7",
        "php-di/php-di": "^6.3",
        "php-di/slim-bridge": "^3.3",
        "silex/silex": "^2.3",
        "slim/http": "^1.2",
        "slim/slim": "^4.8"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^8.5.33",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "*"
    }
}
